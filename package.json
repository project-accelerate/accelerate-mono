{
  "name": "a2",
  "scripts": {
    "start:backend": "yarn stop && docker-compose up --force-recreate --abort-on-container-exit --build",
    "start:frontend": "react-static start",
    "start:storybook": "start-storybook -p 6006",
    "stop": "docker-compose stop && docker-compose rm -f",
    "typecheck": "tsc --noemit",
    "migrations:add": "knex migrate:make -x ts",
    "migrations:run": "knex migrate:latest",
    "update-graphql-schema": "ts-node scripts/generate-types",
    "test:frontend:unit": "jest -c jest.frontendUnitTests.config.json",
    "test:backend:unit": "jest -c jest.backendUnitTests.config.json",
    "test:integration": "yarn stop && docker-compose run --rm web node_modules/.bin/jest --forceExit -c jest.backendIntegrationTests.config.json --runInBand"
  },
  "dependencies": {
    "apollo-client-preset": "^1.0.8",
    "axios": "^0.18.0",
    "core-js": "^2.5.6",
    "date-fns": "^1.29.0",
    "glob": "^7.1.2",
    "graphql": "^0.13.2",
    "graphql-yoga": "^1.13.1",
    "knex": "^0.14.6",
    "knex-postgis": "^0.6.0",
    "lodash": "^4.17.10",
    "pg": "^7.4.3",
    "postcode": "^0.2.5",
    "react": "^16.3.2",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "react-jss": "^8.4.0",
    "react-no-ssr": "^1.1.0",
    "react-router": "^4.2.0",
    "react-static": "^5.9.3",
    "reflect-metadata": "^0.1.12",
    "type-graphql": "^0.11.3",
    "typedi": "^0.7.3",
    "uuid": "^3.2.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@material-ui/core": "^1.0.0-rc.1",
    "@material-ui/icons": "^1.0.0",
    "@storybook/addon-actions": "^3.4.4",
    "@storybook/addon-links": "^3.4.4",
    "@storybook/react": "^3.4.4",
    "@types/enzyme": "^3.1.10",
    "@types/faker": "^4.1.2",
    "@types/glob": "^5.0.35",
    "@types/graphql": "^0.13.1",
    "@types/jest": "^22.2.3",
    "@types/knex": "^0.14.14",
    "@types/knex-postgis": "^0.2.1",
