{
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "setupTestFrameworkScriptFile": "<rootDir>/testSetup.integration.ts",
  "testRegex": "__tests__/.*\\.integrationTest\\.ts?$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "<rootDir>/app/*/external/*",
    "<rootDir>/app/*/web/*"
  ],
  "coverageDirectory": ".coverage"
}
