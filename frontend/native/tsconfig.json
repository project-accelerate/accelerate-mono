{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "target": "es2015",
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "esnext.array",
      "esnext.asynciterable"
    ],
    "typeRoots": ["node_modules/@types", "typings"],
    "jsx": "react-native",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": ["build/**", "tmp/**", "dist/**", ".expo/**"]
}
